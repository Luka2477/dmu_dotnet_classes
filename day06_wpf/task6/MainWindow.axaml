<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="task6.MainWindow"
        Title="task6">
    <DockPanel LastChildFill="True">
        <Button DockPanel.Dock="Left" Click="DockButton_OnClick" VerticalAlignment="Stretch" VerticalContentAlignment="Center">Left</Button>
        <Button DockPanel.Dock="Top" Click="DockButton_OnClick" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">Top</Button>
        <Button DockPanel.Dock="Right" Click="DockButton_OnClick" VerticalAlignment="Stretch" VerticalContentAlignment="Center">Right</Button>
        <Button DockPanel.Dock="Bottom" Click="DockButton_OnClick" HorizontalAlignment="Stretch" HorizontalContentAlignment="Center">Bottom</Button>
        <TextBox Name="TextBoxCenter" HorizontalContentAlignment="Center"></TextBox>
    </DockPanel>
</Window>
